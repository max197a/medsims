// HEADER :: START
section#section-top
header.header
  .container(data-aos='fade-down')
    .header__wrapper
      .hamburger.hamburger--spin(js-hamburger)
        .hamburger-box
          .hamburger-inner
      .header__list
        .header__image
          img(src="img/logotype.png", alt="")
        a.header__item(href="#section-board") About Us
        a.header__item(href="#section-platforms") Medical Advisory Board
        a.header__item(href="#section-contacts") Our Platforms
        a.header__item(href="#section-contacts") Contact Us
      .header__socials
        a(href="https://www.linkedin.com/company/sei-healthcare/" target="_blank")
          img(src="img/in.svg", alt="")
        a(href="https://twitter.com/HealthcareSei" target="_blank")
          img(src="img/tw.svg", alt="")
        a(href="https://www.youtube.com/channel/UCt_3f21oT5Ir5d5zXXa2xMQ/featured?view_as=subscriber" target="_blank")
          img(src="img/yt.svg", alt="")
// HEADER :: END
